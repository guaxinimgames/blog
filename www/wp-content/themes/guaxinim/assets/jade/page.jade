extends index.jade
block main
	main#content(class!="<?php echo odin_classes_page_sidebar(); ?>",tabindex="-1",role="main")
	- while ( have_posts() ) : the_post();
		- get_template_part( 'content', 'page' );
		- if ( comments_open() || get_comments_number() ) :
			- comments_template();
		- endif
	- endwhile

